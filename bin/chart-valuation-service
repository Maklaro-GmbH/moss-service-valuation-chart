#!/usr/bin/env node

const readline = require('readline').createInterface({
  input: process.stdin,
  output: process.stdout
})
const generateChart = require('../generateChart2.js')
const json = require('../tests/data/test3.json')
let output = null

readline.question(``, async (JsonAsString) => {
  output = await generateChart(JsonAsString)
  console.log(output)
  readline.close()
})

return output

